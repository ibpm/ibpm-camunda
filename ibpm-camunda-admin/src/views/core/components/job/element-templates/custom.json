  {
    "name": "Custom Mail",
    "id": "CustomMailTask",
    "appliesTo": [
      "bpmn:ServiceTask"
    ],
    "properties": [
      {
        "label": "Executor Component",
        "type": "String",
        "value": "${customMailTask}",
        "editable": false,
        "binding": {
          "type": "property",
          "name": "camunda:delegateExpression"
        }
      },
      {
        "label": "From",
        "type": "Text",
        "binding": {
          "type": "camunda:inputParameter",
          "name": "customMailFrom"
        },
        "constraints": {
          "pattern": {
            "value": "^[A-z0-9._%+-]+@[A-z0-9.-]+\\.[A-z]{2,}$",
            "message": "Must be a valid email."
          }
        }
      },
      {
        "label": "To",
        "type": "Text",
        "binding": {
          "type": "camunda:inputParameter",
          "name": "customMailTo"
        },
        "constraints": {
          "pattern": {
            "value": "^[A-z0-9._%+-]+@[A-z0-9.-]+\\.[A-z]{2,}$",
            "message": "Must be a valid email."
          },
          "notEmpty": true
        }
      },
      {
        "label": "Subject",
        "type": "Text",
        "binding": {
          "type": "camunda:inputParameter",
          "name": "customMailSubject"
        },
        "constraints": {
          "notEmpty": true
        }
      },
      {
        "label": "Content",
        "value": "Hello ${userName}!",
        "type": "Text",
        "binding": {
          "type": "camunda:inputParameter",
          "name": "customMailContent",
          "scriptFormat": "freemarker"
        },
        "constraints": {
          "notEmpty": true
        }
      }
    ],
    "entriesVisible": {
      "_all": true,
      "delegate": false,
      "implementation": false,
      "fields": false,
      "field-name": false,
      "field-type": false,
      "field-value": false
    }
  },
